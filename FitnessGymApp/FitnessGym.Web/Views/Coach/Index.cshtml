@model IEnumerable<FitnessGym.Models.Coach>

<h1>Fitness Gym Coaches</h1>


<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">start working date</th>
            <th scope="col">Coach Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.StartWorkingDate.ToShortDateString()</td>
                <th>@item.CoachName</th>
            </tr>
        }

    </tbody>
</table>